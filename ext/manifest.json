{
  "name": "Buzzfeed Clickbaiter",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds even more clickbait.",
  "homepage_url": "http://rabrennie.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.buzzfeed.com/*"
      ],
      "js": [
        "src/inject/answers.js",
        "src/inject/titles.js",
        "js/jquery/jquery.min.js",
        "src/inject/inject.js"

      ]
    }
  ]
}